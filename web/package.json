{
  "name": "reearth-marketplace",
  "description": "Re:Earth marketplace for publishing and sharing plugins",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=20.18.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:ext": "tsc && vite build -c vite.config.ext.ts && node ./TEMP",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "type": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "gql": "graphql-codegen --config codegen.yml",
    "i18n": "i18next"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/fragment-matcher": "5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.0.9",
    "@graphql-codegen/typescript-operations": "4.2.3",
    "@graphql-codegen/typescript-react-apollo": "4.3.2",
    "@rollup/plugin-yaml": "4.1.2",
    "@storybook/addon-actions": "8.3.4",
    "@storybook/addon-essentials": "8.3.4",
    "@storybook/addon-links": "8.3.4",
    "@storybook/builder-vite": "8.3.4",
    "@storybook/react": "8.3.4",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.16.10",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@types/react-router-dom": "5.3.3",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "8.8.0",
    "@vitejs/plugin-react": "4.3.2",
    "autoprefixer": "10.4.20",
    "babel-loader": "9.2.1",
    "babel-plugin-macros": "3.1.0",
    "eslint": "9.11.1",
    "eslint-config-reearth": "0.3.6",
    "eslint-plugin-graphql": "4.0.0",
    "i18next-parser": "9.0.2",
    "jsdom": "25.0.1",
    "msw": "2.4.9",
    "prettier": "3.3.3",
    "rollup-plugin-external-globals": "0.12.0",
    "rollup-plugin-visualizer": "5.12.0",
    "storybook-react-i18next": "3.1.7",
    "typescript": "5.6.2",
    "vite": "5.4.8",
    "vite-plugin-css-injected-by-js": "3.5.2",
    "vitest": "2.1.2"
  },
  "dependencies": {
    "@ant-design/compatible": "5.1.3",
    "@ant-design/cssinjs": "1.21.1",
    "@ant-design/icons": "5.5.1",
    "@ant-design/pro-layout": "7.20.2",
    "@ant-design/pro-table": "3.17.2",
    "@apollo/client": "3.11.8",
    "@auth0/auth0-react": "2.2.4",
    "@emotion/css": "11.13.4",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@types/apollo-upload-client": "18.0.0",
    "antd": "5.21.2",
