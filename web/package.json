{
  "name": "reearth-marketplace",
  "description": "Re:Earth marketplace for publishing and sharing plugins",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:ext": "tsc && vite build -c vite.config.ext.ts",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "type": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "gql": "graphql-codegen --config codegen.yml",
    "i18n": "i18next"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-operations": "2.4.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.16",
    "@rollup/plugin-yaml": "^3.1.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/react": "^6.4.22",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.15.0",
    "eslint-config-reearth": "^0.1.0",
    "i18next-parser": "^6.4.0",
    "jsdom": "^19.0.0",
    "msw": "^0.43.1",
    "prettier": "^2.6.2",
    "storybook-react-i18next": "^1.1.1",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vitest": "^0.14.2"
  },
  "dependencies": {
    "@apollo/client": "^3.6.2",
    "@auth0/auth0-react": "^1.10.1",
    "graphiql": "^1.8.10",
    "graphql": "^16.5.0",
    "i18next": "^21.8.2",
    "i18next-browser-languagedetector": "^6.1.4",
    "jotai": "^1.7.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.16.9",
    "react-router-dom": "^6.3.0"
  },
  "msw": {
    "workerDirectory": "src/mocks"
  }
}
