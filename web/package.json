    "graphql": "16.9.0",
    "i18next": "23.15.1",
    "i18next-browser-languagedetector": "8.0.0",
    "jotai": "2.10.0",
    "react": "18.3.1",
    "react-accessible-accordion": "5.0.0",
    "react-dom": "18.3.1",
    "react-i18next": "15.0.2",
    "react-markdown": "9.0.1",
    "react-router-dom": "6.26.2",
    "remark-gfm": "4.0.0",
    "tinycolor2": "1.6.0"
  },
  "msw": {
    "workerDirectory": "src/test/mocks"
  }
}
