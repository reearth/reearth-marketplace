    "i18next-browser-languagedetector": "8.2.0",
    "jotai": "2.15.0",
    "react": "19.2.0",
    "react-accessible-accordion": "5.0.1",
    "react-dom": "19.2.0",
    "react-i18next": "16.0.0",
    "react-markdown": "10.1.0",
    "react-router-dom": "7.9.3",
    "remark-gfm": "4.0.1",
    "tinycolor2": "1.6.0"
  },
  "msw": {
    "workerDirectory": "src/test/mocks"
  }
}
