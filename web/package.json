{
  "name": "reearth-marketplace",
  "description": "Re:Earth marketplace for publishing and sharing plugins",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=22.20.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:ext": "tsc && vite build -c vite.config.ext.ts && node ./TEMP",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "type": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "gql": "graphql-codegen --config codegen.yml",
    "i18n": "i18next"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "6.0.0",
    "@graphql-codegen/fragment-matcher": "6.0.0",
    "@graphql-codegen/introspection": "5.0.0",
    "@graphql-codegen/typescript": "5.0.1",
    "@graphql-codegen/typescript-operations": "5.0.1",
    "@graphql-codegen/typescript-react-apollo": "4.3.3",
    "@rollup/plugin-yaml": "4.1.2",
    "@storybook/addon-actions": "9.0.8",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-links": "9.1.10",
    "@storybook/builder-vite": "9.1.10",
    "@storybook/react": "9.1.10",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.18.8",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@types/react-router-dom": "5.3.3",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "@vitejs/plugin-react": "5.0.4",
    "autoprefixer": "10.4.21",
    "babel-loader": "10.0.0",
    "babel-plugin-macros": "3.1.0",
    "eslint": "9.37.0",
    "eslint-config-reearth": "0.3.8",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "^2.32.0",
    "i18next-parser": "9.3.0",
    "jsdom": "27.0.0",
    "msw": "2.11.3",
    "prettier": "3.6.2",
    "rollup-plugin-external-globals": "0.13.0",
    "rollup-plugin-visualizer": "6.0.4",
    "storybook-react-i18next": "4.0.11",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vite-plugin-css-injected-by-js": "3.5.2",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@ant-design/compatible": "5.1.4",
    "@ant-design/cssinjs": "1.24.0",
    "@ant-design/icons": "6.1.0",
    "@ant-design/pro-layout": "7.22.7",
    "@ant-design/pro-table": "3.21.0",
    "@apollo/client": "4.0.7",
    "@auth0/auth0-react": "2.5.0",
    "@emotion/css": "11.13.5",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
